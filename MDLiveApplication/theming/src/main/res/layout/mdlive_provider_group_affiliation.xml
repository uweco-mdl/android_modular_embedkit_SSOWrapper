<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    style="@style/LinearLayoutStyle">

    <LinearLayout
        style="@style/DoctorLinearLayoutStyle"
        android:background="@drawable/searchpvr_white_rounded_corner"
        android:orientation="vertical">
        <TextView
            style="@style/ProviderDetailHeaderStyle"
            android:text="@string/mdl_affiliations"
            android:tag="@string/mdltag_affiliations"
            />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/providerGroupAffiliationLogo"
            android:layout_gravity="center"
            />
    </LinearLayout>

</LinearLayout>